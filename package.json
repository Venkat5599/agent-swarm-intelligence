{
  "name": "agent-swarm-intelligence",
  "version": "1.0.0",
  "description": "Multi-agent coordination platform where specialized agents work together to solve complex tasks",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "start": "bun run src/index.ts",
    "start:research": "bun run src/agents/ResearchAgent.ts",
    "start:analysis": "bun run src/agents/AnalysisAgent.ts",
    "start:trading": "bun run src/agents/TradingAgent.ts",
    "start:monitor": "bun run src/agents/MonitorAgent.ts",
    "dev": "bun --watch src/index.ts",
    "dashboard": "cd dashboard && bun run dev",
    "dashboard:build": "cd dashboard && bun run build",
    "test": "bun test",
    "test:jupiter": "bun run scripts/test-jupiter.ts",
    "status": "bun run scripts/status.ts",
    "setup-solana": "bun run scripts/setup-solana.ts",
    "fund-wallets": "bun run scripts/fund-wallets.ts",
    "create-project": "bun run scripts/create-swarm-project.ts",
    "submit-project": "bun run scripts/submit-project.ts"
  },
  "keywords": [
    "solana",
    "agent",
    "swarm",
    "coordination",
    "ai",
    "multi-agent"
  ],
  "author": "orchestrator-ai",
  "license": "MIT",
  "dependencies": {
    "@coral-xyz/anchor": "^0.30.0",
    "@solana/web3.js": "^1.98.4",
    "augenpay-sdk": "^1.2.0",
    "bs58": "^6.0.0",
    "cross-fetch": "^4.1.0",
    "dotenv": "^16.4.5",
    "openai": "^6.18.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/node": "^25.2.1",
    "@types/ws": "^8.18.1",
    "bun-types": "^1.3.8"
  }
}
