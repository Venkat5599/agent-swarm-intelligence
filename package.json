  "author": "orchestrator-ai",
  "license": "MIT",
  "dependencies": {
    "@coral-xyz/anchor": "^0.30.0",
    "@solana/web3.js": "^1.95.0",
    "dotenv": "^16.4.5",
    "openai": "^6.18.0",
    "ws": "^8.18.0"
  }
}
